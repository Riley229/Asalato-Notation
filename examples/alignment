\meta {
	\paper {
		\size letter
	}
	\title "Asalato Notation Examples"
	\subtitle "with Western musical notation"
	\composer "J.S. Bach"
}

\score {
	\header "Flip-Flop -> Triplet Den-Den"
	\layout {
		\staff "Player #1"
	}

		\voice "Player #1" {
			\right {
				\time 4/4
		\dotValue 1/8
				FI\knock FO\knock FI+\knock FO+\knock CI\knock CO\knock CI+\knock CO+\knock DD\knock DU\knock DD+\knock DU+\knock AT\knock AF\knock
				FI*1.5 FO*1 AT*1 . | x FI FI FI FI
			}

			\left {
				FO\knock FI+\knock FO+\knock CI\knock CO\knock CI+\knock CO+\knock DD\knock DU\knock DD+\knock DU+\knock AT\knock AF\knock FI*1
				FO*1 AT*0.5 . | x FI\knock FI FI FI FI
			}
		}
}