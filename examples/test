\meta {
	\papersize letter
	\title "Asalato Notation Examples"
	\subtitle "with Western musical notation"
	\composer "J.S. Bach"
}

# currently only supports one score per document
\score {
	\header "Flip-Flop -> Triplet Den-Den"
	\layout {
		# currently only supports one staff per score
		\staff "Player #1" {
			#\includeWesternNotation
		}
	}

	\voice "Player #1" {
		\right {
			\time 4/4
			\dotValue 1/8
			FI\ff..FO\knock....
			FI..FO\staccato....
			\tuplet {
				\duration 4
				FI.FO
			}
			\tuplet {
				\duration 4
				.FI.
			}
			\tuplet {
				\duration 4
				FO.FI
			}
			\tuplet {
				\duration 4
				.FO.
			}
		}

		\left {
			....FI\knock..FO
			....FI\marcato..FO
			\tuplet {
				\duration 4
				.FI.
			}
			\tuplet {
				\duration 4
				FO.FI
			}
			\tuplet {
				\duration 4
				.FO.
			}
			\tuplet {
				\duration 4
				FI.FO
			}
		}
	}
}